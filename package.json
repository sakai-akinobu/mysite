{
  "name": "mysite",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run prerender && npx webpack --config webpack/production.js",
    "dev": "npm run prerender && npx webpack-dev-server --config webpack/development.js",
    "prerender": "npx webpack --config webpack/prerender.js && node ./tmp/prerender.js > public/index.html",
    "compile": "npx tsc --noEmit",
    "lint": "npx eslint src/**/*.tsx",
    "lint:fix": "npx eslint --fix src/**/*.tsx",
    "stylelint": "npx stylelint src/**/*.scss",
    "stylelint:fix": "npx stylelint --fix src/**/*.scss"
  },
  "author": "Akinobu SAKAI <sakai.akinobu@gmail.com> (https://github.com/sakai-akinobu)",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run compile && npm run lint && npm run stylelint"
    }
  },
  "devDependencies": {
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.1",
    "@types/scrollreveal": "0.0.5",
    "@typescript-eslint/eslint-plugin": "1.7.0",
    "autoprefixer": "9.6.5",
    "css-loader": "2.1.1",
    "eslint": "5.16.0",
    "eslint-plugin-react": "7.14.3",
    "file-loader": "3.0.1",
    "husky": "2.2.0",
    "mini-css-extract-plugin": "0.6.0",
    "node-sass": "4.12.0",
    "postcss-loader": "3.0.0",
    "postcss-scss": "2.0.0",
    "sass-loader": "7.1.0",
    "stylelint": "10.0.1",
    "stylelint-config-standard": "18.3.0",
    "svg-url-loader": "2.3.3",
    "ts-loader": "5.4.5",
    "typescript": "3.4.5",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.3.1"
  },
  "dependencies": {
    "github-calendar": "^1.3.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  }
}
